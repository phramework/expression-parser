{
  "name": "phramework/expression-parser",
  "description": "LISP-like expression parser for phramework",
  "license": "Apache-2.0",
  "homepage": "https://phramework.github.io/",
  "type": "project",
  "authors": [
    {
      "name": "Xenofon Spafaridis",
      "email": "nohponex@gmail.com",
      "homepage": "https://nohponex.gr"
    }
  ],
  "require": {
    "php": "^7.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "6.*",
    "squizlabs/php_codesniffer": "*",
    "codacy/coverage": "^1.0",
    "satooshi/php-coveralls": "^2.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Phramework\\ExpressionParser\\": "src"
    }
  },
  "scripts": {
    "test": [
      "phpunit"
    ],
    "lint": "phpcs -p -s --standard=PSR2 ./src",
    "doc": "phpdoc.php run -d ./src -t ./doc"
  }
}
